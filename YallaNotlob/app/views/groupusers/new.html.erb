<%= form_tag("/groupusers/create", method:"get") do  %>
    <div>
      
      <%= text_field_tag(:group_name) %> 

    </div>
    <div>
     <%= submit_tag "add new group", :class => "btn btn-primary", :id => "add" %>
    
    </div>
  <% end %>
 
 
  <ul style="border:1px solid ;width:35%" id="list">
  
  <% @currentgroupusers.each  do |groupuser| %>
    <li>
        <%= groupuser[0].group_name%>

         <%= link_to "Adduser", groupusers_new_path(:account_id => groupuser[0].id),method: :get%>
  <%=link_to 'DElETuser',  groupusers_path(:account_id => groupuser[0].id), method: :delete, data: { confirm: "Are you sure?" } %>
    
    <%= yield %> 

 
      
      
    </li>
  <% end %>
</ul>
 
